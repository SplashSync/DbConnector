<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Databases\OsPosBundle\Entity\OsposTaxCategories" table="ospos_tax_categories">
    <id name="taxCategoryId" type="integer" column="tax_category_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="taxCategory" type="string" column="tax_category" length="32" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="taxGroupSequence" type="boolean" column="tax_group_sequence" nullable="false"/>
  </entity>
</doctrine-mapping>
