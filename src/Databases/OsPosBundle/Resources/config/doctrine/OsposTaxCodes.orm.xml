<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Databases\OsPosBundle\Entity\OsposTaxCodes" table="ospos_tax_codes">
    <id name="taxCode" type="string" column="tax_code" length="32">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="taxCodeName" type="string" column="tax_code_name" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="taxCodeType" type="boolean" column="tax_code_type" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="city" type="string" column="city" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="state" type="string" column="state" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
