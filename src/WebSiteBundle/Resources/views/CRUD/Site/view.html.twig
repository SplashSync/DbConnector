{% extends 'SonataAdminBundle:CRUD:base_show.html.twig' %}
{% import   "SplashBundle:macros:macros.html.twig"   as  Macros %}

{% block show %}
    
    <div class="row">

        <div class="col-lg-5 col-md-4 col-sm-12">

            <div class="bs-docs-section clearfix">
                {{ Macros.Items ( objects,    'Objects' ) }}
                {{ Macros.Items ( widgets,    'Widgets' ) }}
            </div>

        </div>

        <div class="col-lg-7 col-md-8 col-sm-12">

            <div class="bs-docs-section clearfix">
                {{ Macros.TestResult ( results.selftest,    'Self-Test of Splash Module', selftest ) }}
                {{ Macros.TestResult ( results.ping,        'Ping of Splash Server', ping ) }}
                {{ Macros.TestResult ( results.connect,     'Connect of Splash Server', connect ) }}
            </div>

        </div>
    </div>         
            

{#    {{ dump() }}#}
    
{% endblock %}
